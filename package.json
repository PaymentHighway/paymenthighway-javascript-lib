{
  "name": "paymenthighway",
  "version": "2.4.0",
  "description": "Paymenthighway SDK",
  "homepage": "https://www.paymenthighway.fi/dev/",
  "main": "./js/index.js",
  "types": "./js/index.d.ts",
  "scripts": {
    "preversion": "npm run compile && npm test",
    "precompile": "npm run clean",
    "compile": "tsc",
    "test": "set NODE_ENV=test && mocha -t 6000 -R spec js/tests --grep @external --invert",
    "test-all": "set NODE_ENV=test && mocha -t 6000 -R spec js/tests",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -- -t 6000 -R spec js/tests",
    "clean": "del-cli js/",
    "lint": "tslint ts/**/*.ts",
    "pre-commit": "npm run lint"
  },
  "author": {
    "name": "Pauli Kostamo",
    "email": "pauli.kostamo@paymenthighway.fi",
    "url": "http://paymenthighway.fi/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/solinor/paymenthighway-javascript-lib"
  },
  "license": "MIT",
  "dependencies": {
    "@types/lodash": "^4.14.108",
    "@types/node": "^6.0.107",
    "@types/request": "2.47.0",
    "@types/request-promise": "^4.1.33",
    "@types/urijs": "^1.15.37",
    "@types/uuid": "^3.4.3",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "request": "^2.85.0",
    "request-promise": "^4.1.1",
    "uuid": "^3.0.1"
  },
  "engines": {
    "node": ">=4.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/chai-string": "^1.4.1",
    "@types/mocha": "^2.2.33",
    "chai": "^3.5.0",
    "chai-string": "^1.3.0",
    "del-cli": "^1.1.0",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.8.3",
    "urijs": "^1.18.1",
    "zombie": "^5.0.0"
  }
