{
  "name": "paymenthighway",
  "version": "1.1.1",
  "description": "Paymenthighway SDK",
  "homepage": "https://www.paymenthighway.fi/dev/",
  "main": "js/index.js",
  "typings": "js/index.d.ts",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "postinstall": "typings install",
    "preversion": "npm run compile && npm test",
    "precompile": "npm run clean",
    "compile": "tsc",
    "babel": "babel js --out-dir es5",
    "test": "set NODE_ENV=test && mocha -t 6000 -R spec js/tests",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -- -t 6000 -R spec js/tests",
    "clean": "del-cli js/",
    "lint": "tslint ts/**/*.ts"
  },
  "author": {
    "name": "Pauli Kostamo",
    "email": "pauli.kostamo@solinor.com",
    "url": "http://paymenthighway.fi/"
  },
  "repository" : {
    "type" : "git",
    "url" : "https://github.com/solinor/paymenthighway-javascript-lib"
  },
  "license": "MIT",
  "dependencies": {
    "core-js": "^0.6.0",
    "lodash": "^4.15.0",
    "moment": "^2.15.0",
    "node-uuid": "^1.4.7",
    "request": "^2.74.0",
    "request-promise": "^4.1.1",
    "typings": "^1.3.1"
  },
  "engines": {
    "node": ">=4.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "chai": "^3.5.0",
    "chai-string": "^1.2.0",
    "del-cli": "^0.2.0",
    "ghooks": "^1.2.4",
    "istanbul": "^0.4.5",
    "mocha": "^2.5.3",
    "tslint": "^3.12.1",
    "tslint-microsoft-contrib": "^2.0.8",
    "typescript": "^1.8.10",
    "urijs": "^1.18.1",
    "zombie": "^4.2.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint"
    }
  }
}
